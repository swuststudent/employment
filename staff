int main()
{
	CEmployee e1;
	CDate d1;
	int a;
	long i;
	char s,str[19],na[20];
	cout<<"请输入员工姓名:";
	cin>>na;
	cout<<"性别(m代表男性f代表女性)";
	cin>>s;
	cout<<"年龄";
	cin>>a;
	cout<<"工号";
	cin>>i;
	cout<<"身份证号码：";
